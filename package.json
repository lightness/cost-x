{
  "name": "cost-x",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "nest start --watch",
    "data:migrate": "nest start --entryFile migrate-data.entrypoint",
    "graphql:generate:schema": "nest start --entryFile generate-graphql-schema.entrypoint",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:up": "docker compose -f ./docker/compose.yml up -d",
    "migration:create": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:create ./src/database/migrations/${migration_name:-Migration}",
    "migration:generate": "node --require ts-node/register ./node_modules/typeorm/cli.js -d ./src/database/db.config.ts migration:generate ./src/database/migrations/${migration_name:-Migration}",
    "migration:dev:up": "node --require ts-node/register ./node_modules/typeorm/cli.js -d ./src/database/db.config.ts migration:run",
    "migration:dev:down": "node --require ts-node/register ./node_modules/typeorm/cli.js -d ./src/database/db.config.ts migration:revert"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "5.1.0",
    "@as-integrations/express5": "1.1.2",
    "@nestjs/apollo": "13.2.1",
    "@nestjs/axios": "4.0.1",
    "@nestjs/common": "11.1.6",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.6",
    "@nestjs/graphql": "13.2.0",
    "@nestjs/platform-express": "11.1.6",
    "@nestjs/typeorm": "11.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "google-auth-library": "10.4.0",
    "google-spreadsheet": "5.0.2",
    "graphql": "16.11.0",
    "jsonwebtoken": "9.0.2",
    "pg": "8.16.3",
    "typeorm": "0.3.26",
    "typeorm-transactional": "0.5.0"
  },
  "devDependencies": {
    "@types/node": "22.18.0",
    "change-case": "5.4.4",
    "db-migrate": "0.11.14",
    "db-migrate-pg": "1.5.2",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.2"
  }
}
