    "@nestjs/common": "10.2.8",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "10.2.8",
    "@nestjs/platform-express": "10.2.8",
    "@nestjs/typeorm": "10.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "jsonwebtoken": "9.0.2",
    "pg": "8.11.3",
    "typeorm": "0.3.17",
    "typeorm-transactional": "0.5.0"
  },
  "devDependencies": {
    "@types/node": "20.8.10",
    "change-case": "5.4.4",
    "db-migrate": "0.11.14",
