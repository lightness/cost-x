  "main": "index.js",
  "scripts": {
    "start:dev": "nest start --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:up": "docker compose -f ./docker/compose.yml up -d",
    "migration:create": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:create ./src/database/migrations/${migration_name:-Migration}",
    "migration:generate": "node --require ts-node/register ./node_modules/typeorm/cli.js -d ./src/database/db.config.ts migration:generate ./src/database/migrations/${migration_name:-Migration}",
    "migration:dev:up": "node --require ts-node/register ./node_modules/typeorm/cli.js -d ./src/database/db.config.ts migration:run",
    "migration:dev:down": "node --require ts-node/register ./node_modules/typeorm/cli.js -d ./src/database/db.config.ts migration:revert"
  },
